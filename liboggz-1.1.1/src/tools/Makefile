	$(LINK) $(oggz_dump_OBJECTS) $(oggz_dump_LDADD) $(LIBS)
oggz-info$(EXEEXT): $(oggz_info_OBJECTS) $(oggz_info_DEPENDENCIES) 
	@rm -f oggz-info$(EXEEXT)
	$(LINK) $(oggz_info_OBJECTS) $(oggz_info_LDADD) $(LIBS)
