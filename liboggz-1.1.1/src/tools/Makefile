	@rm -f oggz-known-codecs$(EXEEXT)
	$(LINK) $(oggz_known_codecs_OBJECTS) $(oggz_known_codecs_LDADD) $(LIBS)
oggz-merge$(EXEEXT): $(oggz_merge_OBJECTS) $(oggz_merge_DEPENDENCIES) 
	@rm -f oggz-merge$(EXEEXT)
	$(LINK) $(oggz_merge_OBJECTS) $(oggz_merge_LDADD) $(LIBS)
oggz-rip$(EXEEXT): $(oggz_rip_OBJECTS) $(oggz_rip_DEPENDENCIES) 
	@rm -f oggz-rip$(EXEEXT)
	$(LINK) $(oggz_rip_OBJECTS) $(oggz_rip_LDADD) $(LIBS)
